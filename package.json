{
  "title": "Quản lý chứng khoán bằng google sheet",
  "name": "google-sheet",
  "version": "1.0.0",
  "scripts": {
    "build:main": "npm run clean && tsc && npm run removeImport && npm run build:assets && npm run cpMain && npm run build:clasp:main",
    "build:dw": "npm run clean && npx tsc && npm run removeImport && npm run build:assets && npm run cpDW && npm run build:clasp:dw",
    "build:deploy": "npm run deploy",
    "build:assets": "node scripts/build-assets.js",
    "build:clasp:main": "node scripts/build-clasp-main.js",
    "build:clasp:dw": "node scripts/build-clasp-dw.js",
    "clean": "node scripts/clean.js",
    "removeImport": "node scripts/remove-import.js",
    "cpMain": "copyfiles -u 1 -f -E build/src/utility/*.js build/src/projectMain/*.js dist",
    "cpDW": "copyfiles -u 1 -f -E build/src/utility/*.js build/src/projectDw/*.js dist",
    "deploy": "cd dist && clasp push",
    "lint": "gts lint",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm.cmd run compile",
    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run lint",
    "test": "jest -c"
  },
  "description": "Quản lý chứng khoán bằng google sheet",
  "devDependencies": {
    "@types/google-apps-script": "^1.0.83",
    "@types/jest": "^29.5.3",
    "@types/moment": "^2.13.0",
    "@types/node": "20.11.5",
    "cheerio": "^1.0.0-rc.12",
    "copyfiles": "^2.4.1",
    "globals": "^15.0.0",
    "gulp": "^5.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-tap": "^2.0.0",
    "jest": "^29.6.2",
    "moment": "^2.30.1",
    "shelljs": "0.8.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.1.6",
    "upath": "2.0.1"
  },
  "type": "module"
}
