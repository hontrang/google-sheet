{
  "title": "Quản lý chứng khoán bằng google sheet",
  "name": "google-sheet",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run clean && npx tsc && npm run build:assets && npm run build:clasp",
    "build:deploy": "npm run clean && npx tsc && npm run build:assets && npm run deploy",
    "build:assets": "node scripts/build-assets.js",
    "build:clasp": "node scripts/build-clasp.js",
    "clean": "node scripts/clean.js",
    "deploy": "cd dist && clasp push",
    "lint": "gts lint",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm.cmd run compile",
    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run lint"
  },
  "description": "Quản lý chứng khoán bằng google sheet",
  "devDependencies": {
    "@types/google-apps-script": "^1.0.83",
    "@types/moment": "^2.13.0",
    "@types/node": "20.11.5",
    "cheerio": "^1.0.0-rc.12",
    "globals": "^15.0.0",
    "shelljs": "0.8.5",
    "tslib": "^2.6.2",
    "typescript": "~5.4.3",
    "upath": "2.0.1"
  },
  "type": "module"
}
